  <system.web>
    <customErrors mode="Off" defaultRedirect="~/Error">
      <error redirect="#ajax/error404.html" statusCode="404"/>
    </customErrors>
    <globalization enableClientBasedCulture="true" uiCulture="es" culture="es"/>
    <compilation debug="true" targetFramework="4.5.1"/>
    <httpRuntime targetFramework="4.5" maxQueryStringLength="50000" enable="true"/>
